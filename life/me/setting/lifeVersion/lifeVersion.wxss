.version-view {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
}

.info-view {
  margin-top: 200rpx;
  width: 700rpx;
  height: auto;
  margin-left: 25rpx;
  background: rgba(0, 0, 0, 0.4);
  text-align: center;
  border-radius: 20rpx/200rpx;
  padding-bottom: 100rpx;
}

.text {
  width: 100%;
  margin-top: 80rpx;
  height: 80rpx;
  color: white;
  font-size: 30rpx;
  letter-spacing: 8rpx;
}
