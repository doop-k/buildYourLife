page {
  padding: 0rpx;
  margin: 0rpx;
}

.userPageView {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.userHeadView {
  display: flex;
  flex-direction: row;
  height: 400rpx;
  width: 100%;
  border-bottom: 1px solid #b1aaaa;
}

.userImg {
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 200rpx;
  border: 1px solid #4ca8f2;
}

.fadeImg {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 400rpx;
  border-radius: 230rpx;
  border: 1px solid #4ca8f2;
  z-index: -99;
  -webkit-filter: blur(20rpx);
  
}

.d_userNickname {
  width: auto;
  height: 230rpx;
  text-align: center;
  font-size: 50rpx;
  margin-top: 50rpx;
  margin-left: 300rpx;
  letter-spacing: 10rpx;

}

.default_img {
  height: 202rpx;
  width: 209rpx;
  position: absolute;
  top: 204rpx;
  left: 11rpx;
}

.default_textAge {
  margin-top: 200rpx;
  margin-left: 100rpx;
  font-size: 50rpx;
}

.bodyAgeView {
  margin: 10rpx 0rpx 200rpx 68rpx;
  width: 600rpx;
  height: auto;
  padding: 2rpx 5rpx 0rpx 5rpx;
  text-align: center;
  border: 1px solid black;
  border-bottom: none;
}

.ageView {
  font-size: 40rpx;
  letter-spacing: 5rpx;
  width: 100%;
  border-bottom: 1px solid #b1aaaa;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}
