page {
  padding: 0rpx;
  margin: 0rpx;
}

.userPageView {

  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.userHeadView {
  display: flex;
  flex-direction: row;
  height: 400rpx;
  width: 100%;

}

.userImg {
  position: absolute;
  top: 180rpx;
  left: 20rpx;
  width: 200rpx;
  height: 200rpx;
  border-radius: 200rpx;

}

.fadeImg {
  position: absolute;
top:0rpx;
  left: 0rpx;
  width: 100%;
  height: 400rpx;
  z-index: -99;
  -webkit-filter: blur(10rpx);
}

.d_userNickname {
  width: auto;
  height: 230rpx;
  text-align: center;
  font-size: 50rpx;
  margin-top: 200rpx;
  margin-left: 300rpx;
  letter-spacing: 10rpx;
  font-weight:bolder;
}

